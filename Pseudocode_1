INPUT num1
INPUT num2
INPUT num3
IF num1 < num2 THEN
  IF num1 < num3 THEN
     PRINT "The first number is the smallest"
  ELSE 
     PRINT "The third number is the smallest"
  ENDIF
ELSEIF num2 < num3 THEN
  PRINT "The second number is the smallest"
ELSE 
  PRINT"The third number is the smallest"
ENDIF



